PATH=/usr/bin:/usr/sbin

while read src dest
do
	sed \
		-e "s,\%WEBSERVICES_LIB\%,${BASEDIR}/opt/SUNWjax/share/lib," \
		-e "s,//,/,g" \
		< $src > $dest
	echo $dest
done
exit 0
