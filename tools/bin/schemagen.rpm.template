then
    JAVA=$JAVA_HOME/bin/java
else
    JAVA=java
fi

exec $JAVA $XJC_OPTS -cp $WEBSERVICES_LIB/jaxb-xjc.jar com.sun.tools.jxc.SchemaGeneratorFacade "$@"
